{
  "name": "spotify-to-apple-music-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node -r module-alias/register dist/app.js",
    "build": "tsc",
    "dev": "ts-node -r tsconfig-paths/register src/app.ts",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'"
  },
  "_moduleAliases": {
    "@": "dist",
    "$": "."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npm test -- --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^8.45.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.45.0",
    "eslint-plugin-jest": "^28.9.0",
    "globals": "^13.15.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^13.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
